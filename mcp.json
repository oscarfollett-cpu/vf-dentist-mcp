{
  "schema_version": "1.0",
  "name": "dentist_calendar",
  "version": "1.0.0",
  "description": "Google Calendar MCP server for dentist appointment booking.",
  "tools": [
    {
      "name": "check_availability",
      "type": "http",
      "method": "POST",
      "url": "/check",
      "inputSchema": {
        "type": "object",
        "properties": {
          "start": { "type": "string" },
          "end": { "type": "string" }
        },
        "required": ["start", "end"]
      }
    },
    {
      "name": "create_appointment",
      "type": "http",
      "method": "POST",
      "url": "/create",
      "inputSchema": {
        "type": "object",
        "properties": {
          "summary": { "type": "string" },
          "description": { "type": "string" },
          "start": { "type": "string" },
          "end": { "type": "string" },
          "patient": {
            "type": "object",
            "properties": {
              "name": { "type": "string" },
              "email": { "type": "string" },
              "phone": { "type": "string" }
            },
            "required": ["name"]
          }
        },
        "required": ["summary", "start", "end"]
      }
    },
    {
      "name": "update_appointment",
      "type": "http",
      "method": "POST",
      "url": "/update",
      "inputSchema": {
        "type": "object",
        "properties": {
          "eventId": { "type": "string" },
          "start": { "type": "string" },
          "end": { "type": "string" }
        },
        "required": ["eventId", "start", "end"]
      }
    },
    {
      "name": "delete_appointment",
      "type": "http",
      "method": "POST",
      "url": "/delete",
      "inputSchema": {
        "type": "object",
        "properties": {
          "eventId": { "type": "string" }
        },
        "required": ["eventId"]
      }
    }
  ]
}